{
    "manifest_version": 3,
    "name": "Google Classroom 拡張機能",
    "version": "2.0",
    "description": "Google Classroomを使いやすくします。",
    "permissions": ["activeTab"],
    "host_permissions": [
      "https://classroom.google.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://classroom.google.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  